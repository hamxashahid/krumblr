<div class="page-header">
  <%= link_to blogs_path, class: 'btn btn-warning' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_blog_path(@blog), class: 'btn btn-warning' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show blog</h1>
</div>

<dl class="dl-horizontal">

  <dt>Title:</dt>
  <dd><%= @blog.title %></dd>

  <dt>Desc:</dt>
  <dd><%= @blog.desc %></dd>

</dl>
